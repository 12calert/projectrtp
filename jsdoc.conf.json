{
    "plugins": [ "plugins/markdown" ],
    "recurseDepth": 10,
    "source": {
        "include": [ "index.js", "test", "test/interface", "lib" ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}
